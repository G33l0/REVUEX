      "admin", "administrator", "panel", "portal", "cpanel", "webmin", "dashboard",
      "login", "signin", "sso", "auth", "oauth", "cdn", "static", "assets", "img",
      "images", "media", "files", "upload", "uploads", "download", "downloads"
    ]
  },
