  "common_subdomains": {
    "description": "Most common subdomains",
    "wordlist": [
      "www", "mail", "ftp", "localhost", "webmail", "smtp", "pop", "ns1", "ns2",
      "ns3", "ns4", "dns", "dns1", "dns2", "mx", "mx1", "mx2", "api", "dev",
