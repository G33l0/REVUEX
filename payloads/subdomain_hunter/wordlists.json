  "takeover_indicators": {
    "description": "Subdomain takeover indicators by service",
    "services": {
      "github_pages": {
        "cname_patterns": ["github.io", "githubusercontent.com"],
        "error_patterns": ["There isn't a GitHub Pages site here"]
      },
      "heroku": {
        "cname_patterns": ["herokuapp.com", "herokussl.com"],
        "error_patterns": ["No such app", "no-such-app"]
      },
      "aws_s3": {
        "cname_patterns": ["s3.amazonaws.com", "s3-website"],
        "error_patterns": ["NoSuchBucket", "The specified bucket does not exist"]
      },
      "aws_cloudfront": {
        "cname_patterns": ["cloudfront.net"],
        "error_patterns": ["Bad request", "The request could not be satisfied"]
      },
